{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "User ID =postgres;Password=adminpassword;Server=postgres-haproxy;Port=5432;Database=myappdb;Pooling=true;"
  },
  "JWT_KEY": "YourSuperSecretKeyzxcvbnm,asdfghjklqwertyhjrtyu",
  "JWT_ISSUER": "yourdomain.com",
  "JWT_AUDIENCE": "yourdomain.com",
  "ADMIN_USERNAME": "admin",
  "ADMIN_PASSWORD": "1234",

  "Sentry": {
    "Dsn": "cxzcxz",
    "SendDefaultPii": true,
    "MaxRequestBodySize": "Always",
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticLevel": "Error",
    "TracesSampleRate": 1.0,
    "ServerName": "BackEnd",
    "DefaultTags": {
      "default-key-in-config": "BackTag"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Otlp": {
    "ServiceName": "Wunca45.API",
    "Endpoint": "http://otel:4317"
  }
}